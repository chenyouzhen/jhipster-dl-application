<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Product.
    -->
    <changeSet id="20200320030300-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="id_id"
                                 baseTableName="product"
                                 constraintName="fk_product_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product_stock"/>

        <addForeignKeyConstraint baseColumnNames="factory_id"
                                 baseTableName="product"
                                 constraintName="fk_product_factory_id"
                                 referencedColumnNames="id"
                                 referencedTableName="factory"/>

    </changeSet>
</databaseChangeLog>
