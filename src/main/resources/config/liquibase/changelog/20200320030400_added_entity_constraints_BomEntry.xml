<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity BomEntry.
    -->
    <changeSet id="20200320030400-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="id_id"
                                 baseTableName="bom_entry"
                                 constraintName="fk_bom_entry_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="material"/>

        <addForeignKeyConstraint baseColumnNames="product_id_id"
                                 baseTableName="bom_entry"
                                 constraintName="fk_bom_entry_product_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

    </changeSet>
</databaseChangeLog>
