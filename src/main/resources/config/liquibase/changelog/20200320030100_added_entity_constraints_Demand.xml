<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity Demand.
    -->
    <changeSet id="20200320030100-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_id_id"
                                 baseTableName="demand"
                                 constraintName="fk_demand_product_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="demand"
                                 constraintName="fk_demand_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>

    </changeSet>
</databaseChangeLog>
